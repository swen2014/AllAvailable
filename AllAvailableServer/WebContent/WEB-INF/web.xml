<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app id="WebApp_ID">
	<display-name>AllAvailableServer</display-name>
	<servlet>
		<servlet-name>UserMain</servlet-name>
		<display-name>UserMain</display-name>
		<description></description>
		<servlet-class>com.cmu.qiuoffer.Servlet.UserMain</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SeatOperation</servlet-name>
		<display-name>SeatOperation</display-name>
		<description></description>
		<servlet-class>com.cmu.qiuoffer.Servlet.SeatOperation</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Comment</servlet-name>
		<display-name>Comment</display-name>
		<description></description>
		<servlet-class>com.cmu.qiuoffer.Servlet.Comment</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LogIn</servlet-name>
		<display-name>LogIn</display-name>
		<description></description>
		<servlet-class>com.cmu.qiuoffer.Servlet.LogIn</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserMain</servlet-name>
		<url-pattern>/UserMain</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SeatOperation</servlet-name>
		<url-pattern>/SeatOperation</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Comment</servlet-name>
		<url-pattern>/Comment</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogIn</servlet-name>
		<url-pattern>/LogIn</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>
